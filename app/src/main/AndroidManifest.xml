<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kirsh.doc2family">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dtf_logo_2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/dtf_logo_2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.AddPatientActivity"></activity>
        <activity android:name=".views.SignUpActivity" />
        <activity android:name=".views.FriendsActivity" />
        <activity android:name=".views.QuestionsActivity" />
        <activity android:name=".views.PatientInfoActivity" />
        <activity android:name=".views.ListPatientsActivity" />
        <activity android:name=".views.LoginActivity" />
        <activity android:name=".views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>